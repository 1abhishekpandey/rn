{
  "name": "@rudderstack/rudder-sdk-react-native-monorepo",
  "version": "1.5.1",
  "description": "Rudder React Native SDK monorepo",
  "keywords": [
    "react-native",
    "rudder"
  ],
  "author": "RudderStack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudderlabs/rudder-sdk-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-react-native/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-react-native#readme",
  "scripts": {
    "setup": "npm ci && nx sync-deps example && nx ensure-symlink example && cd apps/example/ios && pod install",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx run-many --target=test --parallel=1 && ./node_modules/.bin/lcov-result-merger '**/coverage/lcov.info' 'merged-lcov.info'",
    "test:ci": "nx run-many --target=test --parallel=1 --configuration=ci && ./node_modules/.bin/lcov-result-merger '**/coverage/lcov.info' 'merged-lcov.info'",
    "clean": "rm -rf node_modules/.cache && nx reset && rm -rf reports && rm -rf dist",
    "check:lint": "nx run-many --target=lint --parallel=1",
    "check:lint:ci": "nx run-many --target=lint --parallel=1 --configuration=ci",
    "check:circular": "madge --circular --extensions ts ./libs || exit 0",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "nx lint --configuration=fix",
    "prepare": "husky install",
    "pre-commit": "npm run test && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "release:github": "npx conventional-github-releaser -p angular"
  },
  "private": true,
  "dependencies": {
    "@babel/runtime": "7.20.6",
    "@react-navigation/native": "6.0.14",
    "@react-navigation/native-stack": "6.9.2",
    "@rudderstack/rudder-integration-amplitude-react-native": "file:libs/rudder-integration-amplitude-react-native",
    "@rudderstack/rudder-integration-appcenter-react-native": "file:libs/rudder-integration-appcenter-react-native",
    "@rudderstack/rudder-integration-appsflyer-react-native": "file:libs/rudder-integration-appsflyer-react-native",
    "@rudderstack/rudder-integration-braze-react-native": "file:libs/rudder-integration-braze-react-native",
    "@rudderstack/rudder-integration-clevertap-react-native": "file:libs/rudder-integration-clevertap-react-native",
    "@rudderstack/rudder-integration-firebase-react-native": "file:libs/rudder-integration-firebase-react-native",
    "@rudderstack/rudder-integration-moengage-react-native": "file:libs/rudder-integration-moengage-react-native",
    "@rudderstack/rudder-integration-singular-react-native": "file:libs/rudder-integration-singular-react-native",
    "@rudderstack/rudder-sdk-react-native": "file:libs/sdk",
    "async-lock": "1.4.0",
    "core-js": "3.26.1",
    "react": "18.2.0",
    "react-native": "0.70.6",
    "react-native-safe-area-context": "3.4.1",
    "react-native-screens": "3.18.2",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@commitlint/config-conventional": "17.3.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "7.4.2",
    "@nrwl/cli": "15.2.1",
    "@nrwl/detox": "15.2.1",
    "@nrwl/eslint-plugin-nx": "15.2.1",
    "@nrwl/jest": "15.2.1",
    "@nrwl/linter": "15.2.1",
    "@nrwl/react-native": "15.2.1",
    "@nrwl/web": "15.2.1",
    "@nrwl/workspace": "15.2.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/cli": "9.3.2",
    "@react-native-community/cli-platform-android": "9.3.1",
    "@react-native-community/cli-platform-ios": "9.3.0",
    "@rnx-kit/metro-resolver-symlinks": "0.1.22",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/jest-native": "4.0.13",
    "@testing-library/react-native": "11.5.0",
    "@types/async-lock": "1.3.0",
    "@types/jest": "29.2.3",
    "@types/node": "16.11.7",
    "@types/react": "18.0.25",
    "@types/react-native": "0.70.7",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "babel-jest": "29.3.1",
    "commitizen": "4.2.5",
    "commitlint": "17.3.0",
    "conventional-changelog-cli": "2.2.2",
    "conventional-github-releaser": "3.1.5",
    "detox": "19.12.5",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "8.0.3",
    "jest": "29.3.1",
    "jest-circus": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-react-native": "18.0.0",
    "jest-sonar": "0.2.12",
    "lint-staged": "13.0.3",
    "husky": "8.0.2",
    "lcov-result-merger": "4.0.0",
    "madge": "5.0.1",
    "metro": "0.73.3",
    "metro-babel-register": "0.73.3",
    "metro-react-native-babel-preset": "0.73.3",
    "metro-react-native-babel-transformer": "0.73.3",
    "metro-resolver": "0.73.3",
    "nx": "15.2.1",
    "prettier": "2.8.0",
    "react-native-config": "1.4.11",
    "react-native-dotenv": "3.4.2",
    "react-native-svg": "13.6.0",
    "react-native-svg-transformer": "1.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "overrides": {
    "dot-prop": "5.3.0",
    "jest": "29.3.1",
    "react": "18.2.0",
    "semver-regex": "3.1.4",
    "shell-quote": "1.7.3",
    "trim-newlines": "3.0.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{json,js,md,ts,tsx}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "jiraMode": false,
      "jiraOptional": true,
      "skipScope": false,
      "scopes": [
        "@rudderstack/rudder-integration-amplitude-react-native",
        "@rudderstack/rudder-integration-appcenter-react-native",
        "@rudderstack/rudder-integration-appsflyer-react-native",
        "@rudderstack/rudder-integration-braze-react-native",
        "@rudderstack/rudder-integration-clevertap-react-native",
        "@rudderstack/rudder-integration-firebase-react-native",
        "@rudderstack/rudder-integration-moengage-react-native",
        "@rudderstack/rudder-integration-singular-react-native",
        "@rudderstack/rudder-sdk-react-native",
        "@rudderstack/rudder-sdk-react-native-monorepo",
        "example"
      ],
      "maxHeaderWidth": 120,
      "defaultScope": "@rudderstack/rudder-sdk-react-native-monorepo",
      "defaultType": "feat"
    }
  }
}
