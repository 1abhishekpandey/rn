{
  "name": "rudder-integration-singular-react-native",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/rudder-integration-singular-react-native/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/rudder-integration-singular-react-native",
        "tsConfig": "libs/rudder-integration-singular-react-native/tsconfig.lib.json",
        "project": "libs/rudder-integration-singular-react-native/package.json",
        "entryFile": "libs/rudder-integration-singular-react-native/src/index.ts",
        "external": ["react/jsx-runtime", "react-native"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "assets": [
          {
            "glob": "libs/rudder-integration-singular-react-native/README.md",
            "input": ".",
            "output": "."
          },
          {
            "glob": "libs/rudder-integration-singular-react-native/src",
            "input": ".",
            "output": "."
          },
          {
            "glob": "libs/rudder-integration-singular-react-native/android",
            "input": ".",
            "output": "."
          },
          {
            "glob": "libs/rudder-integration-singular-react-native/ios",
            "input": ".",
            "output": "."
          },
          {
            "glob": "libs/rudder-integration-singular-react-native/rudder-integration-singular-react-native.podspec",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/rudder-integration-singular-react-native/src/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/rudder-integration-singular-react-native/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
